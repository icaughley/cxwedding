<tr>
  <td><%= gift.title %></td>
  <td><%= gift.description %></td>
  <td><%= gift.location %></td>
  <td><%= gift.value %></td>
  <td>
    <div class='btn-group'>
      <%= link_to 'Show', gift, class: 'btn' %>
      <%= link_to 'Edit', edit_gift_path(gift), class: 'btn' %>
      <%= link_to 'Destroy', gift, confirm: 'Are you sure?', method: :delete, class: 'btn' %>
    </div>
  </td>
</tr>